{
  "name": "ipfs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "network-extract": "npx truffle networks --clean && npx tnt extractNetworks",
    "network-inject": "npx tnt injectNetworks",
    "compile": "truffle compile",
    "restore": "npm run compile && npm run network-inject",
    "migrate": "truffle migrate",
    "networks": "truffle networks"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "buffer": "^5.2.1",
    "immutability-helper": "^2.8.1",
    "ipfs-api": "^24.0.2",
    "js-promisify": "^1.3.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "truffle-hdwallet-provider": "^1.0.0-web3one.0",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@gnosis.pm/truffle-nice-tools": "^1.1.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "truffle": "^5.0.0-beta.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
